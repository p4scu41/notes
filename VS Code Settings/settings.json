{
  "[php]": {
    "editor.defaultFormatter": "evgenius33.laravel-pint-fixer"
  },
  "breadcrumbs.enabled": false,
  "cSpell.userWords": [
    "fakerphp",
    "fulltext",
    "Geospatial",
    "inertiajs",
    "Jetstream",
    "larastan",
    "laravel",
    "Livewire",
    "Mailables",
    "mailpit",
    "nunomaduro",
    "oklch",
    "pascual",
    "pestphp",
    "pgsql",
    "phpstan",
    "phpunit",
    "Queueable",
    "tightenco",
    "wayfinder"
  ],
  "css.format.spaceAroundSelectorSeparator": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.guides.bracketPairs": true,
  "editor.lineHeight": 20,
  "editor.minimap.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.suggest.localityBonus": true,
  "editor.suggest.showKeywords": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue": "html",
    "vue-html": "html"
  },
  "eslint.format.enable": true,
  "eslint.validate": [
    "html",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "vue-html"
  ],
  "explorer.autoReveal": false,
  "files.associations": {
    "*.mdx": "markdown"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.blame.editorDecoration.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.hovers.changesDiff": "hunk",
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.runMode": "on-demand",
  "laravel-pint-fixer.pintBinPath": "~/.composer/vendor/bin/pint",
  "laravel-pint-fixer.pintConfigPath": "/Users/pascual/Herd/pint.json",
  "less.format.spaceAroundSelectorSeparator": true,
  "php.suggest.basic": false,
  "phpstan.binCommand": ["~/.composer/vendor/bin/phpstan"],
  "phpstan.checkValidity": true,
  "phpstan.configFile": "~/Herd/phpstan.neon,phpstan.neon,phpstan.neon.dist,phpstan.dist.neon",
  "phpstan.memoryLimit": "2G",
  "playwright.reuseBrowser": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "scm.autoReveal": false,
  "scss.format.spaceAroundSelectorSeparator": true,
  "search.mode": "newEditor",
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "error",
  "typescript.format.semicolons": "insert",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${dirty}${activeFolderMedium}${separator}${rootName}${separator}${profileName}${separator}${activeRepositoryBranchName}",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "Prettier-SQL.keywordCase": "upper"
}
